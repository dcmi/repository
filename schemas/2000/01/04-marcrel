  <dc:creator>The Dublin Core Metadata Initiative</dc:creator>
  <dc:description>The Dublin Core type vocabulary is a simple set of class type declarations 
      intended to facilitate the description and discovery of resources.</dc:description>
  <dc:date>1999-12-24</dc:date>
</dct:Schema>

<rdf:Property rdf:ID = "act">
  <rdfs:label>Actor</rdfs:label>
  <rdfs:description>Use for a person who principally exhibits acting skills in a musical or dramatic presentation or entertainment. </rdfs:description>
  <rdfs:subPropertyOf rdf:resource = "http://rdf.dev.oclc.org/eor/1999/12/23-dcv#contributor" />
  <rdfs:isDefinedBy rdf:resource = "http://rdf.dev.oclc.org/eor/2000/00/04-marcrel" />
</rdf:Property>

<rdf:Property rdf:ID = "adp">
  <rdfs:label>Adapter</rdfs:label>
  <rdfs:description>Use for a person who 1) reworks a musical composition, usually for a different medium, or 2) rewrites novels or stories for motion  pictures or other audiovisual medium. </rdfs:description>
  <rdfs:subPropertyOf rdf:resource = "http://rdf.dev.oclc.org/eor/1999/12/23-dcv#contributor" />
  <rdfs:isDefinedBy rdf:resource = "http://rdf.dev.oclc.org/eor/2000/00/04-marcrel" />
</rdf:Property>

<rdf:Property rdf:ID = "ann">
  <rdfs:label>Annotator</rdfs:label>
  <rdfs:description>Use for a person who writes manuscript annotations on a printed item. </rdfs:description>
  <rdfs:subPropertyOf rdf:resource = "http://rdf.dev.oclc.org/eor/1999/12/23-dcv#contributor" />
  <rdfs:isDefinedBy rdf:resource = "http://rdf.dev.oclc.org/eor/2000/00/04-marcrel" />
</rdf:Property>

